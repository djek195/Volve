{
  "$schema": "https://schemas.wp.org/trunk/theme.json",
  "version": 2,
  "settings": {
    "appearanceTools": true,
    "color": {
      "defaultPalette": false,
      "palette": [
        {
          "slug": "v-white",
          "color": "rgb(255,255,255)",
          "name": "Volve White"
        },
        {
          "slug": "v-general",
          "color": "rgb(36, 218, 196)",
          "name": "Volve General"
        },
        {
          "slug": "v-general-100",
          "color": "rgb(30,179,161)",
          "name": "Volve Custom"
        },
        {
          "slug": "v-gray",
          "color": "rgb(154, 154, 154)",
          "name": "Volve Gray"
        },
        {
          "slug": "v-gray-100",
          "color": "rgb(237,239,241)",
          "name": "Volve Gray"
        },
        {
          "slug": "v-main",
          "color": "rgb(49,49,49)",
          "name": "Volve Main"
        },
        {
          "slug": "v-gradient",
          "color": "rgb(255,255,255)",
          "name": "Volve Primary"
        }
      ],
      "background": true,
      "text": true,
      "link": true,
      "custom": true,
      "defaultGradients": false,
      "gradients": [
        {
          "slug": "v-gradient",
          "gradient": "linear-gradient(315deg, rgb(35,35,35) 0%, rgb(54,56,66) 100%)",
          "name": "Volve Gradient"
        }
      ]
    },
    "blocks": {
      "core/site-title": {
        "color": {
          "defaultPalette": true,
          "palette": [
            {
              "slug": "v-white",
              "color": "rgb(255,255,255)",
              "name": "Volve White"
            },
            {
              "slug": "v-general",
              "color": "rgb(36, 218, 196)",
              "name": "Volve General"
            },
            {
              "slug": "v-gray",
              "color": "rgb(154, 154, 154)",
              "name": "Volve Gray"
            },
            {
              "slug": "v-main",
              "color": "rgb(49,49,49)",
              "name": "Volve Main"
            }
          ],
          "link": true,
          "custom": false,
          "gradients": [],
          "customGradient": false
        }

      },
      "core/cover": {
        "color": {
          "duotone": [],
          "customDuotone": false
        }
      },
      "core/preformatted": {
        "typography": {
          "fontSizes": [],
          "customFontSize": false
        }
      }
    },
    "border": {
      "color": true,
      "radius": true,
      "style": true,
      "width": true
    },
    "typography": {
      "fontFamilies": [
        {
          "fontFamily": "LotaGrotesque, sans-serif",
          "slug": "v-grotesque",
          "name": "Volve Grotesque"
        }
      ],
      "fontSizes": [
        {
          "slug": "small",
          "size": "0.875rem",
          "name": "Small"
        },
        {
          "slug": "regular",
          "size": "1rem",
          "name": "Regular"
        },
        {
          "slug": "medium",
          "size": "1.375rem",
          "name": "Medium"
        },
        {
          "slug": "medium-desktop",
          "size": "1.75rem",
          "name": "Medium Desktop"
        },
        {
          "slug": "large",
          "size": "1.875rem",
          "name": "Large"
        },
        {
          "slug": "x-large",
          "size": "2.188rem",
          "name": "X Large"
        },
        {
          "slug": "gigantic",
          "size": "3.75rem",
          "name": "Gigantic"
        },
        {
          "slug": "x-gigantic",
          "size": "4.063rem",
          "name": "Extra Gigantic"
        }
      ],
      "lineHeight": true,
      "dropCap": true,
      "fontWeight": true,
      "fontStyle": true,
      "textTransform": true,
      "letterSpacing": true,
      "textDecoration": true
    },
    "spacing": {
      "margin": true,
      "padding": true,
      "units": [
        "px",
        "rem",
        "vh",
        "vw",
        "%"
      ],
      "blockGap": true
    }
  },
  "styles": {
    "color": {
      "text": "var(--wp--preset--color--v-main)",
      "background": "var(--wp--preset--color--v-white)"
    },
    "elements": {
      "link": {
        "color": {
          "text": "var(--wp--preset--color--v-general)"
        }
      },
      "h1": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--v-grotesque)",
          "fontSize": "var(--wp--preset--font-size--x-large)",
          "fontWeight": "100",
          "lineHeight": "inherit",
          "textDecoration": "none",
          "textTransform": "none"
        }
      },
      "h2": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--v-grotesque)",
          "fontSize": "var(--wp--preset--font-size--large)",
          "fontWeight": "normal",
          "lineHeight": "inherit",
          "textDecoration": "none",
          "textTransform": "none"
        }
      },
      "h3": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--v-grotesque)",
          "fontSize": "var(--wp--preset--font-size--medium)",
          "fontWeight": "300",
          "lineHeight": "inherit",
          "textDecoration": "none",
          "textTransform": "none"
        }
      }
    },
    "blocks": {
      "core/pullquote": {
        "border": {
          "width": "1px",
          "radius": "40px",
          "style": "solid",
          "color": "var(--wp--preset--color--v-main)"
        }
      },
      "core/site-title": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--v-grotesque)"
        }
      }
    },
    "typography": {
      "fontFamily": "var(--wp--preset--font-family--v-grotesque)",
      "fontSize": "var(--wp--preset--font-size--small)",
      "fontStyle": "normal",
      "fontWeight": "100",
      "lineHeight": "inherit",
      "textDecoration": "none",
      "textTransform": "none"
    },
    "spacing": {
      "margin": {
        "top": "0px",
        "bottom": "0px",
        "left": "0px",
        "right": "0px"
      },
      "padding": {
        "top": "0px",
        "bottom": "0px",
        "left": "0px",
        "right": "0px"
      },
      "blockGap": "4rem"
    }
  },
  "customTemplates": [
    {
      "name": "full-width-page",
      "title": "Full Width Page",
      "postTypes": [
        "page"
      ]
    }
  ]
}